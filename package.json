{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Vue frontend for Somni",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mobile": "echo 'Your app will be available at:' && echo 'http://$(ipconfig getifaddr en0 || hostname -I | awk \"{print \\$1}\"):5173' && echo 'Make sure your phone is on the same WiFi network!' && vite",
    "dev:netlify": "netlify dev",
    "build": "vite build",
    "preview": "vite preview",
    "seed:firestore": "netlify functions:invoke seed-firestore --no-identity",
    "type-check": "vue-tsc --noEmit",
    "tsc": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:frontend": "jest --testPathPattern=src",
    "test:backend": "jest --testPathPattern=netlify"
  },
  "dependencies": {
    "@stripe/stripe-js": "^5.2.0",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "firebase": "^11.0.2",
    "firebase-admin": "^13.0.1",
    "node-fetch": "^2.6.9",
    "pinia": "^3.0.1",
    "stripe": "^17.4.0",
    "vue": "^3.5.13",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.4.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "netlify-cli": "^20.1.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.10",
    "vue-tsc": "^2.0.0"
  }
}
